<div id="bweek" style="position:absolute;display:none" align="right"><button class="bnav" id="bpweek" onclick="moveabscal(-7)" title="[TEXT:previous week]">&lt;</button><button class="bnav" id="bnweek" onclick="moveabscal(7)" title="[TEXT:next week]">&gt;</button></div>
<div id="bday" style="position:absolute;display:none" align="right"><button class="bnav" id="bpday" onclick="moveabscal(-1)" title="[TEXT:previous day]">&lt;</button><button class="bnav" id="bnday" onclick="moveabscal(1)" title="[TEXT:next day]">&gt;</button></div>

